# Site URL (for SEO, sitemap, canonical links)
NEXT_PUBLIC_SITE_URL=https://your-domain.com

# API Configuration

# Use mock API (MSW) instead of real API
# Set to "false" in production
NEXT_PUBLIC_USE_MOCK_API=false

# API Base URL (accessible on both client and server)
# Set to your production API URL in production
NEXT_PUBLIC_BASE_API_URL=https://your-api-url.com

# Google OAuth (if using Gmail login)
# Get your Google Client ID from https://console.cloud.google.com/apis/credentials
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your_google_client_id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your_google_client_secret

# AI Service Configuration
AI_SERVICE_URL=your_ai_service_url
AI_API_KEY=your_ai_api_key

# Database
DATABASE_URL=your_database_url
